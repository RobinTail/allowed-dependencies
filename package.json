{
  "name": "eslint-plugin-allowed-dependencies",
  "description": "ESLint plugin Allowed Dependencies",
  "version": "1.3.2",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinTail/allowed-dependencies.git"
  },
  "homepage": "https://github.com/RobinTail/allowed-dependencies",
  "bugs": "https://github.com/RobinTail/allowed-dependencies/issues",
  "funding": "https://github.com/sponsors/RobinTail",
  "author": {
    "name": "Anna Bocharova",
    "url": "https://robintail.cz"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "scripts": {
    "build": "bun run tsdown",
    "lint": "bun run biome check",
    "pretest": "tsc --noEmit",
    "test": "bun test src",
    "mdfix": "bunx --bun prettier *.md --write",
    "version": "bun run tools/version.ts",
    "prepublishOnly": "bun run lint && bun run test && bun run build"
  },
  "dependencies": {
    "ramda": "^0.32.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "2.2.5",
    "@tsconfig/bun": "^1.0.7",
    "@types/bun": "^1.3.0",
    "@types/ramda": "^0.31.1",
    "@typescript-eslint/rule-tester": "^8.46.0",
    "eslint": "^9.37.0",
    "json-schema-to-ts": "^3.1.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || ^24.0.0"
  },
  "keywords": [
    "modules",
    "eslint",
    "eslint-plugin",
    "import",
    "eslint-rules",
    "dependencies",
    "eslintplugin",
    "restriction",
    "external-dependency",
    "exhaustive",
    "extraneous"
  ]
}
